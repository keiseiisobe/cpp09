import random
import subprocess

EXE = "../PmergeMe"

def test_10():
    for n in range(100):
        num_list = random.sample(range(100), 15)
        str_list = [EXE,]
        for i in num_list:
            str_list.append(str(i))
        res = subprocess.run(str_list, capture_output=True)
        assert b"total comparison: 42" in res.stdout
